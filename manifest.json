{
  "manifest_version": 3,
  "name": "lanhu-flutter",
  "version": "1.0.0",
  "description": "通过蓝湖设计稿自动生成Flutter代码",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/utils.js",
        "scripts/clipboard.min.js",
        "scripts/inject.js"
      ],
      "matches": [
        "https://lanhuapp.com/web/*"
      ]
    }
  ]
}